<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WServletDemoFileuploadWebApp_ID">
	<display-name>Fileupload Sample</display-name>
	<listener>
		<listener-class>
			com.sample.fileupload.DemoContextListener
		</listener-class>
	</listener>
	<servlet>
		<servlet-name>DemoServlet</servlet-name>
		<servlet-class>com.sample.fileupload.DemoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DemoServlet</servlet-name>
		<url-pattern>/app/*</url-pattern>
	</servlet-mapping>
</web-app>